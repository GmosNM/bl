#load "std/print.bl"

EntityKind :: enum {
    Player;
    Enemy;
    Buildin;
};

Entity :: struct {
    kind: EntityKind,
    position: Vec3,
    name: string,
    eq: [4]s32
};

main :: fn () s32 {
    print("\n\n");
    print("%\n", Entity);

    foo : Entity;
    foo.kind = EntityKind.Player;
    foo.position = {:Vec3: 125.15f, 0.53f, 12.19f };
    foo.name = "This is the name";
    foo.eq[0] = 16;
    foo.eq[1] = 43;
    foo.eq[2] = -265;
    foo.eq[3] = 0xF2;

    print("%\n", foo); // name not printed in -r

    print("\n\n");
    return 0;
};