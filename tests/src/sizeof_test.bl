/*
 * Unit test
 * -----------
 * Sizeof buildin test 
 */

extern fn printf(s string, s size_t) i32;

struct A {
  i i32,
  f f64
}

struct B {
  a A,
  i i32
}

enum C i32 {
  Hello
}

enum D i64 {
  Hello
}

fn main() i32 {
  var s_i8 size_t = sizeof(i8);
  var s_i32 size_t = sizeof(i32);
  var s_i64 size_t = sizeof(i64); 
  var s_u8 size_t = sizeof(u8);
  var s_u32 size_t = sizeof(u32);
  var s_u64 size_t = sizeof(u64); 
  var s_size_t size_t = sizeof(size_t); 
  var s_string size_t = sizeof(string); 
  var s_char size_t = sizeof(char); 
  var s_bool size_t = sizeof(bool); 
  var s_f32 size_t = sizeof(f32); 
  var s_f64 size_t = sizeof(f64); 
  var s_ptr size_t = sizeof(ptr); 

  var s_A size_t = sizeof(A); 
  var s_B size_t = sizeof(B); 
  var s_C size_t = sizeof(C); 
  var s_D size_t = sizeof(D); 

  printf("sizeof(i8)     = %luB\n", s_i8);
  printf("sizeof(i32)    = %luB\n", s_i32);
  printf("sizeof(i64)    = %luB\n", s_i64);
  printf("sizeof(u8)     = %luB\n", s_u8);
  printf("sizeof(u32)    = %luB\n", s_u32);
  printf("sizeof(u64)    = %luB\n", s_u64);
  printf("sizeof(size_t) = %luB\n", s_size_t);
  printf("sizeof(string) = %luB\n", s_string);
  printf("sizeof(char)   = %luB\n", s_char);
  printf("sizeof(bool)   = %luB\n", s_bool);
  printf("sizeof(f32)    = %luB\n", s_f32);
  printf("sizeof(f64)    = %luB\n", s_f64);
  printf("sizeof(ptr)    = %luB\n", s_ptr);

  printf("sizeof(A)      = %luB\n", s_A);
  printf("sizeof(B)      = %luB\n", s_B);
  printf("sizeof(C)      = %luB\n", s_C);
  printf("sizeof(D)      = %luB\n", s_D);

  return 0;
}