/*
 * Unit test 7
 * -----------
 * Expressions with method calls and sub expressions.
 */

fn main() i32 {
  var a i32 = 2 + 2 * 12 + 4; // 30
  var b i32 = (2 + 2) * 12 + 4; // 52
  var c i32 = 2 + 2 * (12 + 4); // 34
  var d i32 = (2 + get()) * (12 * (4 + 3)); // 588
  var e i32 = add(a, b) * (10 - 5); // 410  

  if (a != 30 || b != 52 || c != 34 || d != 588 || e != 410) {
    return 1;
  }

  return 0;
}

fn get() i32 {
  return 5;
}

fn add(a i32, b i32) i32 {
  return a + b;
}
