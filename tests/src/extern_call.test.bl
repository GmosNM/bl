#private
qsort_arg_pass :: fn () #test {
    qsort :: fn (base: *s32, nitems: c_size_t, size: c_size_t, compar: *fn (*c_int, *c_int) c_int) #extern "qsort";
    
    arr          := {:[10]s32: 1, 5, 99, 3, 4, 8, 11, 23, 45, 103};
    arr_expected :: {:[10]s32: 1, 3, 4, 5, 8, 11, 23, 45, 99, 103};
    print("before: %\n", arr);
    qsort(arr.ptr, auto arr.len, sizeof(s32), &fn (a: *c_int, b: *c_int) c_int {
        return (@a) - (@b);
    });
    print("after: %\n", arr);

    loop i := 0; i < arr.len; i += 1 {
        test_eq(arr[i], arr_expected[i]);
    }
}
