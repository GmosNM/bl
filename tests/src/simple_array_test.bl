/*
 * Unit test
 * -----------
 * Simple array test 
 */

#load "utest.bl"

struct foo_t {
  x i32,
  y i32
}

fn main() i32 {
  const count i32 = 256;
  var arr i32[count];  
  var i i32;

  while (i < count) {
    arr[i] = i;
    i = i + 1;
  }

  i = 0;
  while (i < count) {
    if (arr[i] != i)
      return 1;
    i = i + 1;
  }

  return 0;
}