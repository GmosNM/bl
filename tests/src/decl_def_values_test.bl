/*
 * Unit test 2
 * -----------
 * Declaration default values. 
 */

#load "utest.bl"

struct A {
  b B
}

struct B {
  c C,
  ptr *void
}

struct C {
  i i32,
  ptr *i32
}

fn main() i32 {
  using utest;

  mut _i8     i8;
  mut _i16    i16;
  mut _i32    i32;
  mut _i64    i64;
  mut _u8     u8;
  mut _u16    u16;
  mut _u32    u32;
  mut _u64    u64;
  mut _f32    f32;
  mut _f64    f64;
  mut _char   char;
  mut _string string;
  mut _bool   bool;


  mut a A;

  assert(a.b.ptr == null);
  assert(a.b.c.i == 0);
  assert(a.b.c.ptr == null);

  uninit mut ua A;

  return 0;
}
