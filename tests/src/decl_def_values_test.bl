/*
 * Unit test 2
 * -----------
 * Declaration default values. 
 */

#load "utest.bl"

struct A {
  b B
}

struct B {
  c C,
  ptr *void
}

struct C {
  i i32,
  ptr *i32
}

fn main() i32 {
  using utest;

  var _i8     i8;
  var _i16    i16;
  var _i32    i32;
  var _i64    i64;
  var _u8     u8;
  var _u16    u16;
  var _u32    u32;
  var _u64    u64;
  var _f32    f32;
  var _f64    f64;
  var _char   char;
  var _string string;
  var _bool   bool;


  var a A;

  assert(a.b.ptr == null);
  assert(a.b.c.i == 0);
  assert(a.b.c.ptr == null);

  uninit var ua A;

  return 0;
}
