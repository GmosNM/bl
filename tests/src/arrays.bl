#load "libc.bl"

main : fn () s32 {
  arr s32[10];
  if arr.count != 10 { return EXIT_FAILURE; }

  i := 0;
  while i < arr.count {
    arr[i] = i;
    i = i + 1;
  }

  i = 0;
  while i < arr.count {
    printf("arr[i] = %d\n", arr[i]);
    i = i + 1;
  }


  foos Foo[10];
  foos.count;

  i = 0;
  while i < foos.count {
    foos[i].i = i;
    i = i + 1;
  }

  i = 0;
  while i < foos.count {
    printf("foos[i].i = %d\n", foos[i].i);
    i = i + 1;
  }
  
  return EXIT_SUCCESS;
};

Foo : struct {
  i s32
};
