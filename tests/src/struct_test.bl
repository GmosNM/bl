extern i32 printf(string s, i32 i);

i32 main() {
  A a;

  a.b.i = 10;
  a.i = 10;

  printf("a.b.i = %d\n", a.b.i);
  printf("a.i = %d\n", a.i);
  printf("a.i + a.b.i = %d\n", a.i + a.b.i);

  return 0;
}

struct A {
  B b,
  i32 i
}

