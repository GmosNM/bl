#load "std/string.bl"

using std;

extern fn printf(format string, s string);

fn main() i32 {
  var str string::string_t;
  string::init(&str);

  string::setc(&str, "hello world");
  printf("str = %s\n", string::c_str(&str));
 
  string::terminate(&str);
  return 0;
}
