#test "type cast" {
  // default type of integer literal is 's32'
  i := 666; 

  // type of the integer literal is changed to u64
  j : u64 = 666;

  // 'j' is implicitly casted to 'u32'
  k : u32 = j;

  // implicit cast on function call
  fn (num: u16) {
  } (j);

  // explicit cast of 'f32' type to 's32'
  l := 1.5f;
  m := cast(s32) l;
};