#load "std/debug.bl"

sum :: fn (nums: ...s32) s32 {
  // nums is slice of s32
  result := 0;
  loop i := 0; i < nums.len; i += 1 {
    result += nums[i];
  }

  return result;
};

#test "vargs" {
  s := sum(10, 20, 30);
  assert(s == 60);

  s = sum(10, 20);
  assert(s == 30);

  s = sum();
  assert(s == 0);
};

