/*
 * Unit test
 * -----------
 * Enums.
 */

extern fn printf(s string, e state_e) i32;

enum state_e {
  S0,
  S00 = 0,
  S10 = 10,
  S11,
  S50 = 50,
  S51,
  S53 = 50 + 3 + 12 + 13,
  S54 = S53 + 1
}

enum names_e string { 
  Tereza = "tereza",
  Milan = "milan"
}

fn main() i32 {
  var s state_e;
  var n names_e;

  if (s != state_e::S0)
     return 1;

  s = state_e::S0;
  if (s != state_e::S0)
     return 2;

  s = state_e::S00;
  if (s != state_e::S00)
     return 3;

  s = state_e::S10;
  if (s != state_e::S10)
     return 4;

  s = state_e::S11;
  if (s != state_e::S11)
     return 5;

  s = state_e::S50;
  if (s != state_e::S50)
     return 6;

  s = state_e::S51;
  if (s != state_e::S51)
     return 6;

  s = state_e::S53;
  if (s != state_e::S53)
     return 7;

  printf("S53 = %d\n", s);

  n = names_e::Tereza;
  if (n != names_e::Tereza)
     return 8;

  n = names_e::Milan;
  if (n != names_e::Milan)
     return 9;

  return 0;
}


