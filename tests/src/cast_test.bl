/*
 * Unit test casting 
 * ----------------
 */

extern fn printf(s string, p *void) i32;
extern fn puts(s string) i32;

struct foo_t {
  i *i32
}

fn main() i32 {
  mut foo foo_t;
  mut i i32;
  foo.i = &i;

  // cannot compile
  if ((&i) == foo.i) {
    puts("same"); 
  } else {
    puts("INVALID!!!"); 
  }

  return 0;
}