/*
 * Unit test casting 
 * ----------------
 */

#load "std/utest.bl"

using std::utest;

module casting {
  struct foo_t {
    i *i32
  }
  
  #test
  fn casting() i32 {
    mut foo foo_t;
    mut i i32;
    foo.i = &i;
  
    // cannot compile
    if ((&i) != foo.i) {
      return FAIL;
    }  

    return SUCCESS;
  }
}