#test "sin" {
    // compare results with libc
    c_sinf :: fn (v: f32) f32 #extern "sinf";
    c_sin  :: fn (v: f64) f64 #extern "sin";

    loop i := 0; i < 3141; i += 1 {
        v :: (cast(f32) i) * 0.001f;
        test_f32_eq(sinf(v), c_sinf(v), 0.000001f);
    }

    loop i := 0; i < 3141; i += 1 {
        v :: (cast(f64) i) * 0.001;
        test_f64_eq(sin(v), c_sin(v), 0.000001);
    }
}
