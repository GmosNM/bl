/*
 * Unit test
 * -----------
 * Simple loop test.
 */

extern i32 printf(string s, i32 i, i32 i);

i32 main() {
  i32 x;
  i32 y;
  i32 pred = 100 * 100;
  i32 count;

  loop {
    if (x == 100) {
      break;
    }

    loop {
      if (y == 100) {
        y = 0;
        break;
      }

      y = y + 1;
      count = count + 1;
    }

    x = x + 1;
  }


  if (count != pred) {
    printf("%d != %d\n", count, pred);
    return 1;
  }

  return 0;
}
