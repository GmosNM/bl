/*
 * Unit test
 * -----------
 * Simple loop test.
 */

extern fn printf(s string, i i32, i2 i32) i32;

fn main() i32 {
  var x i32;
  var y i32;
  var pred i32 = 100 * 100;
  var count i32;

  loop {
    if (x == 100) {
      break;
    }

    loop {
      if (y == 100) {
        y = 0;
        break;
      }

      y = y + 1;
      count = count + 1;
    }

    x = x + 1;

    continue;
    x = x - 1;
  }

  if (count != pred) {
    printf("%d != %d\n", count, pred);
    return 1;
  }

  return 0;
}
