/*
 * Unit test
 * -----------
 * Evaluation test 
 */

const a i32 = 1; // 1
const b i32 = 1 + a; // 2 
const c i32 = 1 + b * b; // 5
const d i32 = 1 + (b / b); // 2

fn main() i32 {
  utest::assert(a == 1); 
  utest::assert(b == 2); 
  utest::assert(c == 5); 
  utest::assert(d == 2); 
  return 0;
}