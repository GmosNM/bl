#test "simple function" {
  func : fn (in Any) {
    if (in.type == TypeKind.SInt) {
      // signed integer value
      val := cast(*s32) in.value;
      assert(*val == 666);
    }
  };

  a := 666;

  // this must be generated implicitly by compiler
  // func(Any { .value = cast(*u8) (&a), .type = typeof(a) });


  tmp Any;
  tmp = Any { .value = cast(*u8) (&a), .type = typeof(a) };


  func(tmp);
};