/* playground */

main :: fn () s32 {
    /*
    arr := {:[10]s32: 1, 2, 3, 4, 5, 6, 7, 8, 9, 0};
    foo(arr);
    */

    color: v4;
    color = {:v4: 0.05f, 0.05f, 0.05f, 1.f};
    print("color = %\n", color);

    arr2 :: {:[10]s32: 1, 2, 3, 4, 5, 6, 7, 8, 9, 0};
    tmp : []s32 : arr2;
    print("tmp = %\n", tmp);

    foo(arr2);

    return 0;
}

foo :: fn (data: []s32) { 
    loop i := 0; i < data.len; i += 1 {
        print("value = %\n", data[i]);
    }
}

