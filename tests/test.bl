/* playground */

#load "experimental/array.bl"

main :: fn () s32 {
    print("%\n", 10);
    print("%\n", *s32);
    print("%\n", 10 + 10);
    print("%\n", **s32);
    print("%\n", "Hello world.");
    print("%\n", foo());
    print("%\n", struct { i: s32} );
    print("%\n", foo );

    arr := array_new(s32);
    loop i := 0; i < 33; i += 1 {
        array_push(arr, 10);
    }
    array_delete(arr);
    
    return 0;
};

foo :: fn () s32 {
    return 10;
};

