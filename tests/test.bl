/* playground */

Foo type : struct {
  i s32, j s32
};

Bar : struct { s32, s32 };

Homo : Foo;
Hello : Homo;
Arr : [10] s32;
Integer : s32;

Enum : enum u8 {
  A,
  B,
  C : 10 + 20,
  D,
  E : 14
};

func : fn (i s32) s32 {
  return i;
};

main : fn () s32 {
  i : func(10);
  hello Hello; // hello is lvalue
  hello.i = 10;
  hello.j = 20;

  bar Bar;
  bar._0;
  bar._1;

  bar_ptr := &bar;

  hello_ptr *Hello;
  hello_ptr.i;
  hello_ptr.j;

  milan struct { s32, s32 };
  milan._0 = 10;
  milan._1 = 10;

  martin struct { i s32, j s32 };
  martin.i = 10;
  martin.j = 10;

  return 0;
};

