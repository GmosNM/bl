main :: fn () s32 {
    compare(10, 20);
    compare("Hello", "Hello");
    return 0;
}

compare :: fn (a: ?T, b: T) bool {
    #if typeinfo(T).kind == TypeKind.Int {
        return a == b;
    } else if T == string {
        return String.compare(a, b);
    } else {
        #error "Invalid type!"
    }
}



