/* playground */

#load "std/core.bl"

main :: fn () s32 {
  a := 10;
  b := 20;

  arr := [3]s32 { a, b, 10 };

  loop i := 0; i < arr.len; i += 1 {
    print_int(arr[i]);
    print_str("\n");
  }

  return 0;
};

foo :: fn (args: ...s32) {  
  print_str("\nPassed var args:\n");
  print_int(args.len);
  print_str("\n\n");
  
  loop i := 0; i < args.len; i += 1 {
    print_int(^args[i]);
  }
};
