/* playground */

#load "std/debug.bl"

main :: fn () s32 {
    i: s32 #noinit;
    print("%\n", i);

    arr: [20]s32;
    print("%\n", arr);

    foo: Foo;
    print("%\n", foo);

    loop i: s32; i < 10; i += 1 {
        print("%\n", i);
    }

    return 0;
}

Foo :: struct {
    bar: Bar;
    b: bool;
    arr: [4]s32;
}

Bar :: struct {
    i: s32;
    j: s32;
}
