/* playground */

#load "std/core.bl"

main :: fn () s32 {
    i := 10;
    foo(i);
    
    return 0;
};

foo :: fn (args: ...) {
    loop i := 0; i < args.len; i += 1 {
        v := args[i];
        if v.type_info.kind == TypeKind.Int {
            print("passed int value\n");
        } else {
            print("passed other type value\n");
        }
    }
};
