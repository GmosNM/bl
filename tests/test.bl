// playground 

File :: *u8;
sym : u8 : 10;

file_open :: fn () (file: File, err: s32) {
    print("Call open file!!!\n");
    return &sym, -1;
}

main :: fn () s32 {
    file :: file_open();
    print("file = %\n", file);
    
    file2, err :: file_open();
    print("file = %\nerr = %\n", file2, err);
    return 0;
}
