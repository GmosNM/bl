/* playground */

#load "std/print.bl"

Fn :: * fn (s32, f32, bool) s32;
test_call :: fn (callback: Fn) #extern;

main :: fn () s32 {
    loop i := 0; i < 100; i += 1 {
        test_call(&callback);
    }

    print("after\n");
    return 0;
};

callback :: fn (i: s32, j: f32, k: bool) s32 {
    print("Passed: %; %; %\n", i, j, k);
    foo();
    return 666;
};

foo :: fn () {
    unreachable;
};