/* testing playground */

extern fn puts(s string) i32;
extern fn printf(s string, i i32) i32;

fn main() i32 {
  mut f10 = #run fctr10();
  mut f15 = #run fctr15();
  printf("factorial 10 = %d\n", f10);
  printf("factorial 15 = %d\n", f15);

  hello();
  return 0;
}

fn hello() {
  foo();
}

fn foo() {
}

fn fctr10() i32 {
  return factorial(10);
}

fn fctr15() i32 {
  return factorial(15);
}

fn factorial(f i32) i32 
{
  if (f == 0)  {
    return 1;
  }

  return (f * factorial(f - 1));
}