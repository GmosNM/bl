/* playground */

main : fn () s32 {
  bar := 2;
  bar = (20 - 5) * bar;

  foo bool := true;
  if (foo) {
    bar = 0;
  } else {
    bar = 1;
  }

  return bar;
};

hello : fn () s32 {
  return 1;
};

/*
fn ()			- fn type
hello : fn () extern; 	- external decl of type fn ()
hello : fn () {}; 	- decl with body
fn (i s32) {} (8); 	- anonymous function with direct call
*/
