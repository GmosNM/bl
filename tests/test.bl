main :: fn () s32 {
    flags: string;
    set_flag(&flags, "hello");    
    return 10;
}

cmp :: fn (a: ?T, b: T) bool {
    is_string :: typeinfo(T).kind == TypeKind.String;
    #if is_string {
        return String.compare(a, b);
    } else {
        #assert(typeinfo(T).kind == TypeKind.Int)
        return a == b;
    }
}

