/* playground */

#load "std/core.bl"

main :: fn () s32 {
  str := "I'm glad to see this message!!!\n";
  tmp := str_to_slice(str.ptr, str.len);
  
  print_str(tmp);
  print_int(666);

  return 0;
};

str_to_slice :: fn (str: *u8, len: usize) []u8 {
  tmp : []u8;
  tmp.ptr = str;
  tmp.len = len;
  return tmp;
};
