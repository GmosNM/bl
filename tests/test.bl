#import "std/table"

main :: fn () s32 {
    tbl: std.Table(s32, s32);
    std.tbl_erase(&tbl, 1); 

    it :: std.tbl_end(&tbl);
    std.tbl_erase(&tbl, it);
    return 0;
}

