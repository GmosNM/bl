setlocale :: fn (category: s32, locale: string_view) {
    C.setlocale(category, std.strtoc(locale));
}


main :: fn () s32 {
    //setlocale(C.LC_ALL, "cs_CZ.utf8");
    win32.SetConsoleOutputCP(win32.CP_UTF8);
    std.write_string(os_stdout(), "Příliš žluťoučký kůň úpěl ďábelské ódy.");
    return 0;
}

