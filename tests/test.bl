/* playground */

#load "std/core.bl"
#load "std/debug.bl"

main :: fn () s32 {
    foo :: fn () {
        print("Hello from foo!!!\n");
    };

    bar :: fn () {
        print("Hello from bar!!!\n");
    };

    ptr : *fn() = &foo;
    ptr();

    ptr = &bar;
    ptr();

    foo();
    bar();

    fn () {
        print("Hello from anonymous!!!\n");
    }();

    ptr = &fn () {
        print("Hello from anonymous called via pointer!!!\n");
    };

    ptr();

    return 0;
};
