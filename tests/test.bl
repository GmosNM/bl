/* playground */

#load "std/core.bl"

main :: fn () s32 {
    foo :: 10; // immutable
    print("foo = %\n", foo);
    
    loop i := 0; i < 10; i += 1 {
        print("Hello world!!!\n");
    }

    bar(true);
    bar(false);

    return 0;
};

bar :: fn (v: bool) s32 {
    if v {
        print("v is true\n");
    } else {
        print("v is false\n");
    }

    return 10;
};

