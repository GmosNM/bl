#load "std/basic.bl"

Days :: enum {
    Sun;
    Mon;
}

main :: fn () s32 {
    foo :: {:Foo: 10, false, null};
    arr :: {:[5]s32: 0, 1, 2, 3, 4};

    print("Hello sailor!!!\n");
    print("i = % and j = %\n", 10, 20);
    print("foo = %\n", foo);
    print("T = %\n", s32);
    print("T = %\n", Foo);
    print("arr = %\n", arr);
    print("Jsem % %\n", "Martin", "Dorazil");
    print("Float = %\n", 120.5f);
    print("Hello %\n", -190);
    print("Double %\n", -120.44);
    print("s64 %\n", cast(s64) -120.44);

    day :: Days.Mon;
    i : s32 = auto day;

    print("i == %\n", i);

    return 0;
}

#private

hello :: fn (v: *Foo, i: *Foo) {
}

Foo :: struct {
    a: s32;
    b: bool;
    c: *Bar;
}

Bar :: struct {
    i: s32
}
