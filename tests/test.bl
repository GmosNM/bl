// this is testing script

extern int puts(string s);
extern int printf(string s, int i);
extern void abort();

int main() {
  int a = 0;

  if (a) {
    puts("a TRUE ");
  }

  if (a == 1) {
    puts("a == 1 TRUE ");
  }

  if (a != 1) {
    puts("a != 1 TRUE ");
  }

  if (a > 0) {
    puts("a > 0 TRUE ");
  }

  if (a < 2) {
    puts("a < 2 TRUE ");
  }

  if (a >= 1) {
    puts("a >= 1 TRUE ");
  }

  if (a <= get_one()) {
    puts("a <= 1 TRUE ");
  }

  assert(a, "test assert");

  return 0;
}

int get_one() {
  return 1;
}

void assert(bool expr, string msg) {
  if (expr == true) {
    puts(msg);
  }
}

