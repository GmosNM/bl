main :: fn () s32 {
    print("hello!");
    range := get_range();
    loop i := 0; i < range; i += 1 {
        func();
    }
    return 0;
}

get :: fn () s32 {
    return 10;
}

get_range :: fn () s32 {
    return 2;
}

func :: fn () {
    loop i := 0; i < 3; i += 1 {
        loop j := 0; j < 2; j += 1 {
            print("foo");
        }
    }

}
