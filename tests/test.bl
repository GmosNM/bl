#load "std/print.bl"

Foo :: struct {
    a: s32;
    b: bool;
}

Days :: enum {
    Sun;
    Mon;
}

main :: fn () s32 {
    foo :: {:Foo: 10, false};
    arr :: {:[5]s32: 0, 1, 2, 3, 4};

    print("Hello sailor!!!\n");
    print("i = % and j = %\n", 10, 20);
    print("foo = %\n", foo);
    print("T = %\n", s32);
    print("T = %\n", Foo);
    print("arr = %\n", arr);
    print("Jsem % %\n", "Martin", "Dorazil");
    print("Float = %\n", 120.5f);
    print("Hello %\n", -190);
    print("Double %\n", -120.44);
    print("s64 %\n", cast(s64) -120.44);

    day :: Days.Sun;
    i : s32 : auto day;

    /*
    a : s64 = 10;
    b : usize;

    a = 10  * auto b;
    */

    return 0;
}