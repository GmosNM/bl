main :: fn () s32 {
    print("s32 = %\n", alignof(s32));
    print("s8 = %\n", alignof(s8));
    print("f64 = %\n", alignof(f64));
    print("[10]usize = %\n", alignof([10]usize));
    print("[10]u8 = %\n", alignof([10]u8));
    print("struct = %\n", alignof(struct { i: bool; j: usize; k: s32; }));
    print("struct = %\n", alignof(struct { i: bool; j: usize; k: s32; l: [10]u8 }));
    print("*struct = %\n", alignof(*struct { i: bool; j: usize; k: s32; l: [10]u8 }));
    return 0;
}
