/* playground */

main :: fn () s32 {
    arr := {:[10]s32: 1, 2, 3, 4, 5, 6, 7, 8, 9, 0};
    foo(arr);

    arr2 :: {:[10]s32: 1, 2, 3, 4, 5, 6, 7, 8, 9, 0};
    foo(arr2);
    
    return 0;
}

foo :: fn (data: []s32) { 
    loop i := 0; i < data.len; i += 1 {
        print("value = %\n", data[i]);
    }
}

