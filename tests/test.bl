/* playground */

load "std/debug.bl"

main :: fn () s32 {
  count :: 10;
  loop i := 0; i < count; i += 1 {
    say_hello();
  }

  return 0;
};

test "kokot" {
  main();
};

say_hello :: fn () {
  putchar(72);
  putchar(101);
  putchar(108);
  putchar(108);
  putchar(111);
  putchar(32);
  putchar(87);
  putchar(111);
  putchar(114);
  putchar(108);
  putchar(100);
  putchar(10);
};

putchar :: fn (c: s32) s32 extern;