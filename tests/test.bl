/* playground */

#load "std/print.bl"

Fn :: * fn (s32, bool, bool);
test_call :: fn (callback: Fn) #extern;

main :: fn () s32 {
    loop i := 0; i < 100; i += 1 {
        print("here!!!\n");
        test_call(&callback);
    }

    return 0;
};

callback :: fn (i: s32, j: bool, k: bool) {
    print("passed %, %, %\n", i, j, k);
};
