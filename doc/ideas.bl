// some syntax and feature ideas

// type
Foo type : struct {
  i s32,
  j s32
};

Foo type : struct { s32, s32 };


foo : Foo { .i = 10, .j = 20 };
foo : Foo { ._0 = 10, ._1 = 20 };

// function 
Func type : fn () {};


// values
foo struct { s32, s32 } : struct { s32, s32 } { ._0 = 10, ._1 = 20 }; 
foo : struct { s32, s32 } { ._0 = 10, ._1 = 20 }; 

typeof(foo) -> struct { s32, s32 }